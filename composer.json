{
  "name": "fetchdocs/fetchdocs-api-php",
  "description": "A PHP wrapper for using the fetchdocs API",
  "keywords": [
    "api", "sdk", "client"
  ],
  "homepage": "https://github.com/fetchdocs/fetchdocs-api-php",
  "license": "MIT",
  "authors": [
    {
      "name": "Fethcods SDK Team",
      "homepage": "https://fetchdocs.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=7.0.0",
    "guzzlehttp/guzzle": "^6.3",
    "ext-json": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*"
  },
  "autoload": {
    "psr-4": {
      "Fetchdocs\\SDKClient\\": "src"
    },
    "files": ["./src/constants.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Fetchdocs\\SDKClient\\Test\\": "tests"
    }
  }
}